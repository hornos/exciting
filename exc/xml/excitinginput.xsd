                            type="xs:integer" default="5">
                            <xs:annotation>
                                <xs:appinfo>
                                    <oldname>lmaxmatxs</oldname>
                                </xs:appinfo>
                                <xs:documentation>angular momentum cut-off for the outer-most loop in the hamiltonian and overlap matrix setup</xs:documentation>
                            </xs:annotation>
                        </xs:attribute>
